{
  "name": "svelte-app",
  "version": "1.0.0",
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.2",
    "@tsconfig/svelte": "^3.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^16.18.16",
    "babel-jest": "^29.5.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-fetch-mock": "^3.0.3",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "mini-css-extract-plugin": "^1.3.4",
    "svelte": "^3.55.0",
    "svelte-check": "^3.0.0",
    "svelte-jester": "^2.3.2",
    "svelte-loader": "^3.1.0",
    "svelte-preprocess": "^5.0.2",
    "svelte-preprocess-less": "^0.4.0",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.0.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.0",
    "webpack": "^5.16.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^4.12.0"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "dev": "webpack serve",
    "validate": "svelte-check",
    "test": "jest",
    "test:watch": "npm test --watch"
  },
  "jest": {
    "transform": {
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": true
        }
      ],
      "^.+\\.ts$": "ts-jest",
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "svelte"
    ]
  }
}
